{
  "compilerOptions": {
    "baseUrl": "app",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": [
      "es6",
      "es2017"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "target": "esnext",
    "paths": {
      "@app/*": [
        "*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ],
}